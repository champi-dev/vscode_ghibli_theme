  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8FBC8F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#FFB6C1"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#2D5016",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#696969"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#A0522D"
      }
    },
    {
      "name": "Class/Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#483D8B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Property",
      "scope": [
        "support.type.property-name",
        "meta.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#2D5016"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Import/Require",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#6B5B95"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#8B4513",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#2D5016",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#36454F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#36454F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#87CEEB",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#A0522D"
      }
    },
    {
      "name": "JSON Property Name",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#2D5016"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "strikethrough"
      }
    }
  ]
}